{
  "expo": {
    "name": "EvenTop",
    "slug": "eventop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.jpg",
    "scheme": "eventop",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "usesAppleSignIn": true,
      "supportsTablet": true,
      "bundleIdentifier": "dev.eventop.wallet",
      "associatedDomains": [
        "webcredentials:eventop.xyz"
      ],
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "dev.eventop.wallet"
    },
    "extra": {
      "privyAppId": "cmi91u7ca00csl20dktx7r3cr",
      "privyClientId": "client-WY6TC6KG4fD76kZaKeFdzimsWXyNSnRvgjMdeJS2H8p2d",
      "passkeyAssociatedDomain": "https://eventop.xyz",
      "router": {},
      "eas": {
        "projectId": "2ceafbb7-4d27-4db3-b894-eec149952c15"
      }
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-apple-authentication",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "17.5"
          },
          "android": {
            "compileSdkVersion": 35
          }
        }
      ],
      "expo-font",
      "expo-web-browser",
      "expo-quick-actions",
      {
       "androidIcons": {
          "help_icon": {
            "foregroundImage": "./assets/images/adaptive-icon-help.svg",
            "backgroundColor": "#29cfc1"
          },
          "add_funds_icon": {
            "foregroundImage": "./assets/images/adaptive-icon-add-funds.svg",
            "backgroundColor": "#29cfc1"
          },
          "subscriptions_icon": {
            "foregroundImage": "./assets/images/adaptive-icon-help-subscription.svg",
            "backgroundColor": "#29cfc1"
          }
        }
      }
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "jussec"
  }
}
